<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="YellowMoonShopPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>hannq.entities.Ordering</class>
    <class>hannq.entities.Category</class>
    <class>hannq.entities.User</class>
    <class>hannq.entities.Cake</class>
    <class>hannq.entities.OderDetail</class>
    <class>hannq.entities.TblCake</class>
    <class>hannq.entities.TblCategory</class>
    <class>hannq.entities.TblOrdering</class>
    <class>hannq.entities.TblOderDetail</class>
    <class>hannq.entities.TblUser</class>
    <class>hannq.entities.OrderDetail</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:sqlserver://localhost:1433;databaseName=YellowMoonShop"/>
      <property name="javax.persistence.jdbc.user" value="sa"/>
      <property name="javax.persistence.jdbc.driver" value="com.microsoft.sqlserver.jdbc.SQLServerDriver"/>
      <property name="javax.persistence.jdbc.password" value="123456"/>
    </properties>
  </persistence-unit>
</persistence>
